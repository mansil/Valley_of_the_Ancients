#. [option]: speaker=narrator
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:692 Valley_of_the_Ancients/scenarios/VOTA/05_Out_of_the_Frying_Pan.cfg:307 Valley_of_the_Ancients/scenarios/VOTA/06_Last_Battle.cfg:263
msgid "Saurian"
msgstr ""

#. [option]: speaker=narrator
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:705 Valley_of_the_Ancients/scenarios/VOTA/05_Out_of_the_Frying_Pan.cfg:333 Valley_of_the_Ancients/scenarios/VOTA/06_Last_Battle.cfg:289
msgid "Goblin"
msgstr ""

#. [message]: race=dwarf
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:734
msgid "No! Our sacred treasures, all gone!"
msgstr ""

#. [then]: id=Mal-Draugur
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:739
msgid "Mal-Draugur"
msgstr ""

#. [message]: id=Mal-Draugur
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:748
msgid "Hah hah hah! Foolish mortals... your infighting has only left you weak and vulnerable. Now I will finally destroy you once and for all!"
msgstr ""

#. [message]
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:752
msgid "Pull back! We need a more defensible position to fight from!"
msgstr ""

#. [message]
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:756
msgid "Damn! Too many enemies... we'll have to retreat!"
msgstr ""

#. [message]: race=dwarf
#: Valley_of_the_Ancients/scenarios/VOTA/04_Dwarven_Hold.cfg:763
msgid "Fools! You have doomed us all... below us lies an even greater danger, but now you have left us all no choice."
msgstr ""

